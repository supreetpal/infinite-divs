{
  "name": "infinite-divs",
  "version": "1.0.0",
  "description": "Render an infinite list of divs without crashing the browser.",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "concurrently --kill-others 'npm run ex-dev' 'npm run lib'",
    "ex-dev": "cd examples && node server.js",
    "ex-prod": "cd examples && webpack --config webpack.prod.config.js && cp -r ./images ./dist/images",
    "lib": "babel src -w -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supreetpal/infinite-divs.git"
  },
  "keywords": [
    "infinite",
    "divs",
    "render",
    "performance"
  ],
  "author": "@supreetpal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supreetpal/infinite-divs/issues"
  },
  "homepage": "https://github.com/supreetpal/infinite-divs#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "concurrently": "^2.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
